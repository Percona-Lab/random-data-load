CREATE TABLE t1(
	id int auto_increment primary key
);
CREATE TABLE t2(
	id int auto_increment primary key,
	t1_id int,
	FOREIGN KEY(t1_id) REFERENCES t1(id)
);
CREATE TABLE t3(
	id int auto_increment primary key,
	t2_id int,
	FOREIGN KEY(t2_id) REFERENCES t2(id)
);
CREATE TABLE t4(
	id int auto_increment primary key,
	t2_id int,
	t3_id int,
	FOREIGN KEY(t2_id) REFERENCES t2(id),
	FOREIGN KEY(t3_id) REFERENCES t3(id)
);
